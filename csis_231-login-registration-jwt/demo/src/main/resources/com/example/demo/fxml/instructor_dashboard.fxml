<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/17.0.2-ea"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.demo.instructor.InstructorDashboardController"
      spacing="12" alignment="CENTER"
      stylesheets="@../styles.css">
    <padding>
        <Insets top="24" right="24" bottom="24" left="24"/>
    </padding>
    <Label fx:id="welcomeLabel" styleClass="title" text="Welcome, Instructor!"/>
    <Label text="Instructor dashboard is coming soon."/>
    <Button text="Logout" onAction="#onLogout"/>
</VBox>
